{
	"name": "WPtouch",
	"namemsg": "skinname-wptouch",
	"version": "1.2",
	"author": [
		"Dale Mugford",
		"Duane Storey",
		"Dan Flores",
		"Jack Phoenix"
	],
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"url": "https://www.mediawiki.org/wiki/Skin:WPtouch",
	"license-name": "GPL-2.0-or-later",
	"descriptionmsg": "wptouch-desc",
	"type": "skin",
	"ValidSkinNames": {
		"wptouch": {
			"class": "SkinWPtouch",
			"args": [
				{
					"name": "wptouch",
					"styles": [
						"mediawiki.skinning.interface",
						"mediawiki.skinning.content.externallinks",
						"skins.wptouch"
					],
					"scripts": [
						"skins.wptouch.js"
					]
				}
			]
		}
	},
	"MessagesDirs": {
		"SkinWPtouch": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"SkinWPtouch": "SkinWPtouch.php",
		"WPtouchTemplate": "WPtouchTemplate.php",
		"WPtouchHooks": "WPtouchHooks.php"
	},
	"Hooks": {
		"OutputPageBodyAttributes": "WPtouchHooks::onOutputPageBodyAttributes"
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "WPtouch"
	},
	"ResourceModules": {
		"skins.wptouch": {
			"styles": {
				"resources/css/main.css": {
					"media": "screen"
				},
				"resources/css/print.css": {
					"media": "print"
				},
				"resources/css/really-small.css": {
					"media": "all and (max-width: 350px)"
				}
			}
		},
		"skins.wptouch.js": {
			"scripts": [
				"resources/javascript/core.js",
				"resources/javascript/url-bar.js"
			]
		}
	},
	"manifest_version": 1
}
